{
  "compilerOptions": {
    "jsx": "react-jsx",
    "sourceMap": false,
    "outDir": "lib/types",
    "strict": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "isolatedModules": true,
  },
  "include": [
    "src/**/*"
  ],
  "target": "esnext",
  "module": "esnext",
}
