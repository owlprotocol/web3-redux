{
    "compilerOptions": {
        "jsx": "react-jsx",
        "sourceMap": false,
        "outDir": "lib",
        "strict": true,
        "lib": ["esnext", "dom"],
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "skipLibCheck": true,
        "downlevelIteration": true,
        "isolatedModules": true
    }
}
