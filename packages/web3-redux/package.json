{
  "name": "@owlprotocol/web3-redux",
  "version": "1.0.1-beta.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/web3-redux"
  },
  "homepage": "https://owlprotocol.github.io/web3-redux",
  "copyright": "Copyright 2021 Owl Protocol",
  "author": {
    "name": "Leo Vigna",
    "email": "leo.vigna@gmail.com",
    "url": "https://github.com/leovigna/"
  },
  "license": "MIT",
  "type": "commonjs",
  "main": "./dist/web3-redux-lib.cjs.js",
  "module": "./dist/web3-redux-lib.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/web3-redux-lib.cjs.js",
      "import": "./dist/web3-redux-lib.es.js"
    }
  },
  "files": [
    "dist/*",
    "lib/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "npm-run-all -p clean:lib clean:types",
    "clean:lib": "rimraf lib",
    "clean:types": "rimraf src/types/ethers src/types/truffle src/types/web3",
    "test": "mocha 'lib/cjs/**/*.test.js'",
    "build": "npm-run-all generate-types tsc esbuild",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "generate-types": "npm-run-all -p generate-types:web3",
    "generate-types:web3": "typechain --target=web3-v1 --out-dir=src/types/web3 'src/abis/*.json'",
    "hardhat:compile": "npx hardhat compile",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm run test",
    "publish": "pnpm publish --no-git-checks --access public",
    "madge": "madge src/index.ts -c"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.1",
    "@ipld/dag-pb": "^2.1.16",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/chai-spies": "^1.0.3",
    "axios": "0.25.0",
    "blockstore-core": "^1.0.5",
    "dexie": "^3.2.2",
    "ethereum-ens": "^0.8.0",
    "events": "^3.3.0",
    "indexeddbshim": "^10.0.0",
    "ipfs-http-client": "56.0.3",
    "ipfs-unixfs": "^6.0.9",
    "ipfs-unixfs-exporter": "^7.0.11",
    "ipfs-unixfs-importer": "^9.0.10",
    "iso-url": "^1.2.1",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "multiformats": "^9.6.5",
    "redux": "4.1.2",
    "redux-orm": "^0.16.2",
    "redux-saga": "^1.1.3",
    "tiny-invariant": "1.2.0",
    "typed-redux-saga": "^1.3.1",
    "uuid": "^8.3.2",
    "web3": "1.7.0"
  },
  "peerDependencies": {
    "dexie-react-hooks": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@esbuild-plugins/node-resolve": "^0.1.4",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.7.0",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^6.1.2",
    "@typechain/truffle-v5": "^5.0.0",
    "@typechain/web3-v1": "^3.0.0",
    "@types/chai": "^4.2.21",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.179",
    "@types/mocha": "^9.1.0",
    "@types/moxios": "^0.4.15",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/sinon": "^10.0.12",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "cross-env": "^7.0.3",
    "dexie-react-hooks": "^1.1.1",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.25",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-babel": "^0.2.3",
    "esbuild-plugin-copy": "^1.1.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ganache-core": "2.13.2",
    "glob": "^7.2.0",
    "hardhat": "^2.10.0",
    "ipfs": "0.62.3",
    "jsdom": "^20.0.0",
    "lint-staged": "^11.1.2",
    "madge": "^5.0.1",
    "mocha": "^9.0.3",
    "mocha-jsdom": "^2.0.0",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux-saga-test-plan": "^4.0.3",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "ts-node": "^10.7.0",
    "typechain": "^5.1.2",
    "typescript": "4.6.2",
    "web3-core": "1.7.0",
    "web3-core-subscriptions": "1.7.0",
    "web3-eth": "1.7.0",
    "web3-eth-abi": "1.7.0",
    "web3-eth-contract": "1.7.0",
    "web3-utils": "1.7.0"
  }
}
