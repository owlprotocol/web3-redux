{
  "name": "@owlprotocol/web3-redux",
  "version": "0.1.10-beta.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/web3-redux.git"
  },
  "homepage": "https://owlprotocol.github.io/web3-redux",
  "copyright": "Copyright 2021 Owl Protocol",
  "author": {
    "name": "Leo Vigna",
    "email": "leo.vigna@gmail.com",
    "url": "https://github.com/leovigna/"
  },
  "license": "MIT",
  "files": [
    "*"
  ],
  "publishConfig": {
    "directory": "lib"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "browser": "./dist/index.js",
      "require": "./cjs/index.js"
    }
  },
  "type": "commonjs",
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "npm-run-all -p clean:lib clean:types",
    "clean:lib": "rimraf lib",
    "clean:types": "rimraf src/types/ethers src/types/truffle src/types/web3",
    "test": "mocha 'lib/cjs/**/*.test.js'",
    "start": "node lib/index.js",
    "build": "npm-run-all generate-types tsc copy esbuild",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "copy": "npm-run-all copy:prepublish",
    "copy:prepublish": "cp package.json lib/ && cp README.md lib/ && cp LICENSE lib/",
    "generate-types": "npm-run-all -p generate-types:web3",
    "generate-types:web3": "typechain --target=web3-v1 --out-dir=src/types/web3 'src/abis/*.json'",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm run test",
    "publish": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@ipld/dag-pb": "^2.1.15",
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "0.25.0",
    "ethereum-ens": "^0.8.0",
    "events": "^3.3.0",
    "fast-memoize": "^2.5.2",
    "lodash": "^4.17.21",
    "multiformats": "^9.6.3",
    "path-browserify": "^1.0.1",
    "re-reselect": "^4.0.0",
    "redux": "4.1.2",
    "redux-batched-actions": "^0.5.0",
    "redux-orm": "^0.16.2",
    "redux-persist": "6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "4.1.5",
    "tiny-invariant": "1.2.0",
    "typed-redux-saga": "^1.3.1",
    "url": "^0.11.0",
    "web3": "1.7.0",
    "web3-core": "1.7.0",
    "web3-core-subscriptions": "1.7.0",
    "web3-eth": "1.7.0",
    "web3-eth-abi": "1.7.0",
    "web3-eth-contract": "1.7.0",
    "web3-utils": "1.7.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@esbuild-plugins/node-resolve": "^0.1.4",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/truffle-v5": "^5.0.0",
    "@typechain/web3-v1": "^3.0.0",
    "@types/chai": "^4.2.21",
    "@types/lodash": "^4.14.179",
    "@types/mocha": "^9.1.0",
    "@types/moxios": "^0.4.15",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.25",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-babel": "^0.2.3",
    "esbuild-plugin-copy": "^1.1.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ganache-core": "2.13.2",
    "glob": "^7.2.0",
    "lint-staged": "^11.1.2",
    "mocha": "^9.0.3",
    "mocha-jsdom": "^2.0.0",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux-saga-test-plan": "^4.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typechain": "^5.1.2",
    "typescript": "4.6.2"
  }
}
