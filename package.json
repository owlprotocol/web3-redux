{
    "name": "@leovigna/web3-redux",
    "version": "0.1.5-beta.2",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/leovigna/web3-redux.git"
    },
    "homepage": "https://leovigna.github.io/web3-redux",
    "copyright": "Copyright 2021 Leo Vigna",
    "author": {
        "name": "Leo Vigna",
        "email": "leo.vigna@gmail.com",
        "url": "https://github.com/leovigna/"
    },
    "license": "MIT",
    "scripts": {
        "clean": "npm-run-all -p clean:lib clean:types",
        "clean:lib": "rimraf lib",
        "clean:types": "rimraf src/types/ethers src/types/truffle src/types/web3",
        "test": "mocha 'lib/**/*.test.js'",
        "start": "node lib/index.js",
        "build": "npm-run-all generate-types tsc copy babel",
        "build:watch": "npm-run-all -p tsc:watch babel:watch",
        "tsc": "tsc",
        "tsc:watch": "tsc -w",
        "babel": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps true --copy-files",
        "babel:watch": "babel src -w --out-dir lib --extensions \".ts,.tsx\" --source-maps true --copy-files",
        "copy": "npm-run-all copy:prepublish",
        "copy:prepublish": "cp package.json lib/ && cp README.md lib/ && cp LICENSE lib/",
        "generate-types": "npm-run-all -p generate-types:web3",
        "generate-types:web3": "typechain --target=web3-v1 --out-dir=src/types/web3 'src/abis/*.json'",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "lint:staged": "lint-staged",
        "ganache": "ganache-cli --blockTime 1",
        "docs": "npm-run-all -p docs:serve docs:watch",
        "docs:build": "typedoc && cp -r docs-static/* docs",
        "docs:watch": "nodemon --watch src -e ts --exec 'npm run docs:build'",
        "docs:serve": "serve docs",
        "docs:watch2": "typedoc --watch --preserveWatchOutput",
        "prepare": "husky install && npm run build",
        "prepublishOnly": "npm run test"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.6.2",
        "fast-memoize": "^2.5.2",
        "lodash": "^4.17.21",
        "object-hash": "^2.2.0",
        "re-reselect": "^4.0.0",
        "redux": "^4.1.1",
        "redux-batched-actions": "^0.5.0",
        "redux-orm": "^0.16.2",
        "redux-saga": "^1.1.3",
        "reselect": "^4.0.0",
        "tiny-invariant": "^1.1.0",
        "web3": "^1.6.1",
        "web3-core": "^1.6.1",
        "web3-core-subscriptions": "^1.6.1",
        "web3-eth": "^1.6.1",
        "web3-eth-abi": "^1.6.1",
        "web3-eth-contract": "^1.6.1",
        "web3-utils": "^1.6.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.8",
        "@babel/plugin-transform-runtime": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@babel/runtime": "^7.15.4",
        "@jambit/eslint-plugin-typed-redux-saga": "^0.4.0",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@typechain/ethers-v5": "^7.0.1",
        "@typechain/truffle-v5": "^5.0.0",
        "@typechain/web3-v1": "^3.0.0",
        "@types/chai": "^4.2.21",
        "@types/lodash": "^4.14.176",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.4.13",
        "@types/react": "^17.0.16",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.29.0",
        "@typescript-eslint/parser": "^4.29.0",
        "babel-plugin-macros": "^3.1.0",
        "chai": "^4.3.4",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "ganache-cli": "^6.12.2",
        "ganache-core": "^2.13.2",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "mocha": "^9.0.3",
        "mocha-jsdom": "^2.0.0",
        "moxios": "^0.4.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "redux-saga-test-plan": "^4.0.3",
        "rimraf": "^3.0.2",
        "typechain": "^5.1.2",
        "typed-redux-saga": "^1.3.1",
        "typedoc": "^0.22.10",
        "typescript": "4.4.4"
    },
    "files": [
        "*",
        "!test",
        "!./**/*.test.*",
        "!abis",
        "abis/Multicall.json",
        "!types/ethers",
        "!types/truffle",
        "!types/web3"
    ],
    "main": "index.js",
    "types": "index.d.ts",
    "publishConfig": {
        "directory": "lib"
    }
}
